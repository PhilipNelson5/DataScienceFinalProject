rarity": "uncommon", "rulings": [], "scryfallId": "4dc24eb0-83a1-4373-8678-8fec69fb5def", "scryfallIllustrationId": "ef94c0c4-02c9-44f9-9d6b-0cd2a1a6a9a5", "scryfallOracleId": "0f6e66d5-4f27-485b-999d-ee55c1e218b9", "subtypes": [], "supertypes": [], "tcgplayerProductId": 79334, "text": "Target creature gets +X/+0 until end of turn.", "type": "Instant", "types": ["Instant"], "uuid": "eb048c98-dcb7-5460-8131-f1b99d9da8bd"}, {"artist": "Alan Pollack", "borderColor": "black", "colorIdentity": [], "colors": [], "convertedManaCost": 2.0, "count": 2, "edhrecRank": 8204, "flavorText": "Though no longer attached to the hand, it still holds its adversary in its grasp.", "foreignData": [], "frameVersion": "2003", "hasFoil": false, "hasNonFoil": true, "isFoil": false, "isMtgo": true, "isPaper": true, "isReprint": true, "isStarter": true, "layout": "normal", "legalities": {"commander": "Legal", "duel": "Legal", "legacy": "Legal", "modern": "Legal", "vintage": "Legal"}, "manaCost": "{2}", "mcmId": 240445, "mcmMetaId": 1582, "mtgoId": 34270, "name": "Dragon's Claw", "number": "92", "prices": {"mtgo": {}, "mtgoFoil": {}, "paper": {"2019-11-09": 0.4}, "paperFoil": {}}, "printings": ["10E", "9ED", "DDG", "DPA", "DST", "M10", "M11", "M12", "PS11"], "purchaseUrls": {"cardmarket": "https://mtgjson.com/links/f9b9262027a0b28e", "tcgplayer": "https://mtgjson.com/links/a887ed114e666a5f"}, "rarity": "uncommon", "rulings": [{"date": "2009-10-01", "text": "The ability triggers whenever any player, not just you, casts a red spell."}, {"date": "2009-10-01", "text": "If a player casts a red spell, Dragon’s Claw’s ability triggers and is put on the stack on top of that spell. Dragon’s Claw’s ability will resolve (causing you to gain 1 life) before the spell does."}], "scryfallId": "e81eb75d-e027-4fe6-a440-d438f6f7734a", "scryfallIllustrationId": "c9b19e87-1db2-4f16-afc6-9fade70f6316", "scryfallOracleId": "bd8d707d-1201-42c6-bce6-fbda12ce5ea8", "subtypes": [], "supertypes": [], "tcgplayerProductId": 79386, "text": "Whenever a player casts a red spell, you may gain 1 life.", "type": "Artifact", "types": ["Artifact"], "uuid": "d5a6a7bb-64aa-5d83-bcd9-bb228fcd8981"}, {"artist": "Terese Nielsen & Ron Spencer", "borderColor": "black", "colorIdentity": ["R"], "colors": ["R"], "convertedManaCost": 6.0, "count": 1, "edhrecRank": 5303, "flavorText": "Vengeance is a dish best served twice.", "foreignData": [], "frameVersion": "2003", "hasFoil": false, "hasNonFoil": true, "isFoil": false, "isMtgo": true, "isPaper": true, "isReprint": true, "isStarter": true, "layout": "normal", "legalities": {"commander": "Legal", "duel": "Legal", "legacy": "Legal", "modern": "Legal", "penny": "Legal", "vintage": "Legal"}, "manaCost": "{4}{R}{R}", "mcmId": 240496, "mcmMetaId": 9667, "mtgoId": 34105, "name": "Rage Reflection", "number": "52", "prices": {"mtgo": {}, "mtgoFoil": {}, "paper": {"2019-11-09": 1.91}, "paperFoil": {}}, "printings": ["DPA", "PS11", "SHM"], "purchaseUrls": {"cardmarket": "https://mtgjson.com/links/f9a4b45652462a05", "tcgplayer": "https://mtgjson.com/links/68e379b2c4531dcf"}, "rarity": "rare", "rulings": [{"date": "2008-05-01", "text": "If a creature loses double strike after assigning damage in the “first strike” combat damage step (due to Rage Reflection leaving the battlefield, for example), that creature won’t assign damage in the “normal” combat damage step."}], "scryfallId": "1900c0b1-b2b0-4561-ae63-ab7cd4c54580", "scryfallIllustrationId": "581cb494-cd37-44a8-8372-fca28338a0d7", "scryfallOracleId": "03fb26b1-6d9c-455b-9b2f-a052a9222db7", "subtypes": [], "supertypes": [], "tcgplayerProductId": 79343, "text": "Creatures you control have double strike.", "type": "Enchantment", "types": ["Enchantment"], "uuid": "bdb0fccd-bc46-5c8f-9fe9-0a0818434ec0"}, {"artist": "John Matson", "borderColor": "black", "colorIdentity": ["R"], "colors": ["R"], "convertedManaCost": 4.0, "count": 1, "edhrecRank": 1558, "flavorText": "The furnace awaits the next master who would stoke the fires of apocalypse.", "foreignData": [], "frameVersion": "2003", "hasFoil": false, "hasNonFoil": true, "isFoil": false, "isMtgo": true, "isPaper": true, "isReprint": true, "isStarter": true, "layout": "normal", "legalities": {"commander": "Legal", "duel": "Legal", "legacy": "Legal", "modern": "Legal", "penny": "Legal", "vintage": "Legal"}, "manaCost": "{1}{R}{R}{R}", "mcmId": 240462, "mcmMetaId": 2240, "mtgoId": 33979, "name": "Furnace of Rath", "number": "44", "prices": {"mtgo": {}, "mtgoFoil": {}, "paper": {"2019-11-09": 3.09}, "paperFoil": {}}, "printings": ["10E", "8ED", "9ED", "DPA", "HOP", "PS11", "TMP"], "purchaseUrls": {"cardmarket": "https://mtgjson.com/links/c0804ce439ef2dcc", "tcgplayer": "https://mtgjson.com/links/d221ac3af289c3eb"}, "rarity": "rare", "rulings": [{"date": "2004-10-04", "text": "If a spell or ability damages multiple things, divide up the damage before applying this effect. This means you can’t normally end up with an odd amount of damage on something."}, {"date": "2004-10-04", "text": "The trample rules cause damage to be divided before it is doubled."}, {"date": "2004-10-04", "text": "If you have two of these on the battlefield, the damage is multiplied by 4."}, {"date": "2004-10-04", "text": "The multiplied damage counts in all ways as if it came from the original source. Furnace of Rath is not the source."}, {"date": "2005-08-01", "text": "If multiple effects modify how damage will be dealt, the player who would be dealt damage or the controller of the creature that would be dealt damage chooses the order to apply the effects. For example, Mending Hands says, “Prevent the next 4 damage that would be dealt to any target this turn.” Suppose a spell would deal 5 damage to a player who has cast Mending Hands targeting themselves. That player can either (a) prevent 4 damage first and then let Furnace of Rath double the remaining 1 damage, taking 2 damage, or (b) double the damage to 10 and then prevent 4 damage, taking 6 damage."}], 